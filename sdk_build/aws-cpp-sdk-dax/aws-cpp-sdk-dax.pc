prefix=/usr/local
exec_prefix=${prefix}
includedir=${prefix}/include
libdir=${exec_prefix}/lib

Name: aws-cpp-sdk-dax
Description: C++ SDK for the AWS dax service
Version: 1.4.43
Cflags: -I${includedir} -fno-exceptions -std=c++11
Libs: -L${libdir} -laws-cpp-sdk-dax 
Libs.private: -lpthread -lcurl -lcrypto -lssl -lz
Requires:  aws-cpp-sdk-core
